{
  "name": "volt-for-dynamodb",
  "version": "0.0.0",
  "description": "A lightweight client library for AWS DynamoDB. See README.md for more.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "mise ensure-local-services && tsup --watch",
    "test": "mise ensure-local-services && vitest run",
    "test:watch": "mise ensure-local-services && vitest",
    "test:coverage": "mise ensure-local-services && vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "pnpm run typecheck && pnpm run lint && pnpm run test && pnpm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [
    "dynamodb-toolbox",
    "electrodb",
    "dynamoose",
    "mongoose",
    "orm",
    "sdk",
    "aws",
    "dynamo",
    "dynamodb",
    "database",
    "mongodb",
    "nosql",
    "typescript"
  ],
  "author": {
    "name": "Tej Birring"
  },
  "license": "MIT",
  "engines": {
    "node": ">=24.12.0"
  },
  "packageManager": "pnpm@10.27.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.974.0",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@commitlint/types": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-sh": "^0.18.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.974.0",
    "zod": "^4.3.6"
  }
}
